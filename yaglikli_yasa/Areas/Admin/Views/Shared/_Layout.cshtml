<!doctype html>
<html lang="tr">

<head>
    <meta charset="utf-8" />
    <title>Saglam indir - @ViewBag.Title</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta content="Saglam indir admin paneli" name="description" />
    <meta content="Ozbasoft" name="author" />
    <!-- App favicon -->
    <!-- DataTables -->
    <link href="~/Areas/Admin/assets/libs/datatables.net-bs4/css/dataTables.bootstrap4.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Areas/Admin/assets/libs/datatables.net-buttons-bs4/css/buttons.bootstrap4.min.css" rel="stylesheet" type="text/css" />
    <!-- Responsive datatable examples -->
    <link href="~/Areas/Admin/assets/libs/datatables.net-responsive-bs4/css/responsive.bootstrap4.min.css" rel="stylesheet" type="text/css" />

    <link href="~/Areas/Admin/assets/libs/dropzone/min/dropzone.min.css" rel="stylesheet" type="text/css" />


    <!-- Bootstrap Css -->
    <link href="~/Areas/Admin/assets/css/bootstrap.min.css" id="bootstrap-style" rel="stylesheet" type="text/css" />
    <!-- Icons Css -->
    <link href="~/Areas/Admin/assets/css/icons.min.css" rel="stylesheet" type="text/css" />
    <!-- App Css-->
    <link href="~/Areas/Admin/assets/css/app.min.css" id="app-style" rel="stylesheet" type="text/css" />

</head>

<body data-sidebar="dark">


    <!-- Begin page -->
    <div id="layout-wrapper">

        <header id="page-topbar">
            <div class="navbar-header">
                <div class="d-flex">
                    <!-- LOGO -->


                    <div class="navbar-brand-box">
                        <a href="index-2.html" class="logo logo-dark">
                            <span class="logo-sm">
                                <img src="assets/images/sdasd-sm.png" alt="" height="22">
                            </span>
                            <span class="logo-lg">
                                <img src="assets/images/asdasdsd-dark.png" alt="" height="17">
                            </span>
                        </a>
                        <a href="index-2.html" class="logo logo-light">
                            <span class="logo-sm">
                                <img src="assets/images/asdsadsd-sm.png" alt="" height="22">
                            </span>
                            <span class="logo-lg">
                                <img src="assets/images/sdsd-light.png" alt="" height="18">
                            </span>
                        </a>
                    </div>

                    <button type="button" class="btn btn-sm px-3 font-size-24 header-item waves-effect vertical-menu-btn">
                        <i class="mdi mdi-menu"></i>
                    </button>
                    <div class="d-none d-sm-block">
                        <div class="dropdown dropdown-topbar pt-3 mt-1 d-inline-block">
                            <a class="btn btn-light dropdown-toggle" href="#" role="button" id="dropdownMenuLink" data-bs-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                Oluştur <i class="mdi mdi-chevron-down"></i>
                            </a>
                            <div class="dropdown-menu" aria-labelledby="dropdownMenuLink">
                                <a class="dropdown-item" href="~/admin/bloglars/Create">Blog Ekle</a>
                                <a class="dropdown-item" href="~/admin/kategoris/Create">Kategori Ekle</a>
                                <a class="dropdown-item" href="~/admin/Kullanicis/Create">Kullanıcı Ekle</a>
                                <a class="dropdown-item" href="~/admin/sagliktabloes/Create">Sağlık Ekle</a>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="d-flex">
                    <!-- App Search-->
                    <div class="dropdown d-none d-lg-inline-block">
                        <button type="button" class="btn header-item noti-icon waves-effect" data-toggle="fullscreen">
                            <i class="mdi mdi-fullscreen font-size-24"></i>
                        </button>
                    </div>

                    <div class="dropdown d-inline-block">
                        <button type="button" class="btn header-item waves-effect" id="page-header-user-dropdown"
                                data-bs-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <img class="rounded-circle header-profile-user" src="~/Areas/Admin/assets/images/users/user-4.jpg"
                                 alt="Header Avatar">
                        </button>
                        <div class="dropdown-menu dropdown-menu-end">
                            <!-- item-->
                            <a class="dropdown-item" href="~/Hesap/Cikis"><i class="mdi mdi-lock-open-outline font-size-17 text-muted align-middle me-1"></i> Çıkış Yap</a>
                        </div>
                    </div>

                </div>
            </div>
        </header>
        <!-- ========== Left Sidebar Start ========== -->
        <div class="vertical-menu">
            <div data-simplebar class="h-100">
                <!--- Sidemenu -->
                <div id="sidebar-menu">
                    <!-- Left Menu Start -->
                    <ul class="metismenu list-unstyled" id="side-menu">
                        <li class="menu-title">Menü</li>
                        <li>
                            <a href="~/admin" class="waves-effect">
                                <i class="mdi mdi-view-dashboard"></i>
                                <span class="badge rounded-pill bg-primary float-end"></span>
                                <span>Anasayfa</span>
                            </a>
                        </li>
                        <li>
                            <a href="~/admin/bloglars" class=" waves-effect">
                                <i class="mdi mdi-blogger"></i>
                                <span>Bloglar</span>
                            </a>
                        </li>
                        <li>
                            <a href="~/admin/sagliktabloes" class=" waves-effect">
                                <i class="mdi mdi-blogger"></i>
                                <span>Sağlık</span>
                            </a>
                        </li>
                        <li>
                            <a href="javascript: void(0);" class="has-arrow waves-effect">
                                <i class="ion ion-ios-list-box"></i>
                                <span>Kategoriler</span>
                            </a>
                            <ul class="sub-menu" aria-expanded="false">
                                <li><a href="~/admin/kategoris">Kategoriler</a></li>
                                <li><a href="~/admin/kategoris/Create">Kategori Ekle</a></li>
                            </ul>
                        </li>
                        <li class="menu-title">Ayarlar</li>
                        <li>
                            <a href="~/admin/Kullanicis" class=" waves-effect">
                                <i class="ion ion-md-people"></i>
                                <span>Kullanıcı Ayarları</span>
                            </a>
                        </li>
                        <li>
                            <a href="~/admin/Ayarlars" class=" waves-effect">
                                <i class="mdi mdi-database-settings"></i>
                                <span>Ayarlar</span>
                            </a>
                        </li>
                    </ul>
                </div>
                <!-- Sidebar -->
            </div>
        </div>
        <!-- Left Sidebar End -->
        <!-- ============================================================== -->
        <!-- Start right Content here -->
        <!-- ============================================================== -->
        <div class="main-content">
            <div class="page-content">
                <div class="container-fluid">
                    <!-- start page title -->


                    @RenderBody()

                </div>
                <!-- container-fluid -->
            </div>
            <!-- End Page-content -->




            <footer class="footer">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-sm-12">
                            ©
                            <script>document.write(new Date().getFullYear())</script> Saglam indir <span class="d-none d-sm-inline-block"> - Yönetim Paneli <i class="mdi mdi-heart text-danger"></i> Bütün Haklarımız Saklıdır.</span>
                        </div>
                    </div>
                </div>
            </footer>
        </div>
        <!-- end main content-->
    </div>
    <!-- END layout-wrapper -->
    <!-- Right bar overlay-->
    <div class="rightbar-overlay"></div>
    <!-- JAVASCRIPT -->

    @RenderSection("scripts", required: false)

    <script src="~/Areas/Admin/assets/libs/jquery/jquery.min.js"></script>
    <script src="~/Areas/Admin/assets/libs/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/Areas/Admin/assets/libs/metismenu/metisMenu.min.js"></script>
    <script src="~/Areas/Admin/assets/libs/simplebar/simplebar.min.js"></script>
    <script src="~/Areas/Admin/assets/libs/node-waves/waves.min.js"></script>
    <script src="~/Areas/Admin/assets/libs/jquery-sparkline/jquery.sparkline.min.js"></script>



    <script src="~/Areas/Admin/assets/libs/morris.js/morris.min.js"></script>
    <script src="~/Areas/Admin/assets/libs/raphael/raphael.min.js"></script>
    <script src="~/Areas/Admin/assets/js/pages/dashboard.init.js"></script>
    <script src="~/Areas/Admin/assets/libs/tinymce/tinymce.min.js"></script>
    <script src="~/Areas/Admin/assets/js/pages/form-editor.init.js"></script>
    <script src="~/Areas/Admin/assets/js/app.js"></script>


    <!-- Required datatable js -->
    <script src="~/Areas/Admin/assets/libs/datatables.net/js/jquery.dataTables.min.js"></script>
    <script src="~/Areas/Admin/assets/libs/datatables.net-bs4/js/dataTables.bootstrap4.min.js"></script>
    <!-- Buttons examples -->
    <script src="~/Areas/Admin/assets/libs/datatables.net-buttons/js/dataTables.buttons.min.js"></script>
    <script src="~/Areas/Admin/assets/libs/datatables.net-buttons-bs4/js/buttons.bootstrap4.min.js"></script>
    <script src="~/Areas/Admin/assets/libs/jszip/jszip.min.js"></script>
    <script src="~/Areas/Admin/assets/libs/pdfmake/build/pdfmake.min.js"></script>
    <script src="~/Areas/Admin/assets/libs/pdfmake/build/vfs_fonts.js"></script>
    <script src="~/Areas/Admin/assets/libs/datatables.net-buttons/js/buttons.html5.min.js"></script>
    <script src="~/Areas/Admin/assets/libs/datatables.net-buttons/js/buttons.print.min.js"></script>
    <script src="~/Areas/Admin/assets/libs/datatables.net-buttons/js/buttons.colVis.min.js"></script>
    <script src="~/Areas/Admin/assets/libs/dropzone/min/dropzone.min.js"></script>
    <!-- Responsive examples -->
    <script src="~/Areas/Admin/assets/libs/datatables.net-responsive/js/dataTables.responsive.min.js"></script>
    <script src="~/Areas/Admin/assets/libs/datatables.net-responsive-bs4/js/responsive.bootstrap4.min.js"></script>
    <!-- Datatable init js -->
    <script src="~/Areas/Admin/assets/js/pages/datatables.init.js"></script>
</body>


</html>